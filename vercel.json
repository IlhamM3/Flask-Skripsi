{
    "version": 2,
    "builds": [
        {
            "src": "requirements.txt",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "api/withProba.py"
        }
    ],
    "env": {
        "PYTHON_VERSION": "3.10"
    }
}
